/*---------------------Setup Function-----------------------*/
/*-------------------------------------------------------*/
void setup() {
  int pos = 0;
  Serial.begin(9600); // Starting Serial Terminal
  pinMode(pingPin, OUTPUT);
  pinMode(echoPin, INPUT);
  pinMode(12, OUTPUT);  //pin12 is used as GND pin  for buzzer since arduino nano has only two GND pins
  pinMode(A5, OUTPUT);  //pin A5 provides the output on buzzer
  pinMode(A3, OUTPUT);  //pin A5 provides the output on buzzer

}

/*---------------------Microsecond to Centimeter----------------------------------*/
long microsecondsToCentimeters(long microseconds) {
  return microseconds / 29 / 2;
}

/*------------------------Loop Function-------------------------------*/
void loop() {

  long duration, cm;
  digitalWrite(12, LOW);   //Buzzer GND is always low


  //send a signal at ping pin at an interval of 0.002 seconds to check for an object
  digitalWrite(pingPin, LOW);
  delayMicroseconds(2);
  digitalWrite(pingPin, HIGH);
  delayMicroseconds(10);
  digitalWrite(pingPin, LOW);


  duration = pulseIn(echoPin, HIGH);  //check time using pulseIn function

  cm = microsecondsToCentimeters(duration);   //functin call to find distance

  Serial.print(cm);
  Serial.print("cm");
  Serial.println();
  delay(100);
  /*--------------------------For obstacle between 30 and 50 cm-----------------------------*/
  if (cm < 50 && cm > 30)
  { analogWrite(A5, 255);
    analogWrite(A3, 255);
    analogWrite(A4, 255);
    startPlayback(obstacle, sizeof(obstacle));
    delay(1000);
    analogWrite(A4, 0);
    analogWrite(A5, 0);
    analogWrite(A3, 0);
    delay(1000);
  }    //sound buzzer every second if obstacle distance is between 20-30cm.

  /*--------------------------For obstacle between 30 and 10 cm-----------------------------*/
  else if (cm < 30 && cm > 10) {
    analogWrite(A5, 255);
    analogWrite(A4, 255);
    analogWrite(A3, 255);
    startPlayback(becareful, sizeof(becareful));
    delay(500);
    analogWrite(A5, 0);
    analogWrite(A3, 255);
    analogWrite(A4, 0);
    delay(500);
  }   //sound buzzer every 0.5 seconds if obstacle distance is between 10-20cm.

  /*--------------------------For obstacle between 10 and 0 cm-----------------------------*/
  else if (cm < 10 && cm > 0)  {
    analogWrite(A5, 255);
    analogWrite(A3, 255);
    analogWrite(A4, 255);
    startPlayback(alert, sizeof(alert));
    delay(100);
    analogWrite(A5, 0);
    analogWrite(A3, 255);
    delay(100);
  }    //sound buzzer every 0.1 seconds if obstacle distance is between 0-10cm.

  /*--------------------------For No Obstacle -----------------------------*/
  else                   {
    analogWrite(A5, 0); //do not sound the buzzer
    analogWrite(A3, 0);
    analogWrite(A4, 0);
  }
}
